{
  "providers": {
    "deepseek": {
      "name": "DeepSeek",
      "provider_type": "deepseek",
      "enabled": true,
      "api_base": "https://api.deepseek.com/v1",
      "model": "deepseek-chat",
      "max_tokens": 4000,
      "temperature": 0.1,
      "cost_per_1k_input": 0.00014,
      "cost_per_1k_output": 0.00028,
      "rate_limit": {
        "requests_per_minute": 100,
        "tokens_per_minute": 500000
      },
      "features": {
        "field_matching": true,
        "response_generation": true,
        "form_analysis": true,
        "context_understanding": true
      }
    },
    "local": {
      "name": "Local Model",
      "provider_type": "local",
      "enabled": false,
      "model": "local",
      "max_tokens": 2048,
      "temperature": 0.1,
      "cost_per_1k_input": 0.0,
      "cost_per_1k_output": 0.0,
      "features": {
        "field_matching": false,
        "response_generation": false,
        "form_analysis": false,
        "context_understanding": false
      }
    },
    "basic_matching": {
      "name": "Basic Text Matching",
      "provider_type": "basic_matching",
      "enabled": true,
      "model": "basic",
      "cost_per_1k_input": 0.0,
      "cost_per_1k_output": 0.0,
      "features": {
        "field_matching": true,
        "response_generation": false,
        "form_analysis": false,
        "context_understanding": false
      }
    }
  },
  "default_provider": "deepseek",
  "fallback_provider": "basic_matching",
  "cost_tracking": {
    "enabled": true,
    "monthly_budget": 10.0,
    "alert_threshold": 0.8,
    "currency": "USD"
  },
  "performance": {
    "cache_enabled": true,
    "cache_ttl_hours": 24,
    "batch_processing": true,
    "max_batch_size": 10
  }
}